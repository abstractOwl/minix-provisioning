# !/bin/sh

# Aliases for git
# Inspired by the Oh-My-Zsh git plugin
# <https://github.com/robbyrussel/oh-my-zsh/plugins/git>

alias g="git"
alias ga="git add"
alias gb="git branch"
alias gc="git commit"
alias gcm="git commit -m"
alias gco="git checkout"
alias gd="git diff"
alias gf="git fetch"
alias gm="git merge"
alias gp="git push"
alias gpull="git pull"
alias gr="git reset"
alias grb="git rebase"
alias gs="git status"
